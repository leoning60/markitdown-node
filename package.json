{
  "author": "leoning",
  "name": "markitdown-node",
  "version": "1.5.0",
  "description": "TypeScript document extraction library inspired by markitdown. Converts PDF, DOCX, PPTX, XLSX, HTML, CSV, JSON, XML, RSS, Atom, ZIP, Jupyter Notebooks, Bing SERP, images (PNG, JPEG, TIFF with OCR), subtitles (VTT, SRT), and YouTube videos to JSON and Markdown formats.",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "keywords": [
    "document",
    "converter",
    "pdf",
    "docx",
    "pptx",
    "xlsx",
    "html",
    "csv",
    "json",
    "xml",
    "rss",
    "atom",
    "zip",
    "youtube",
    "jupyter",
    "ipynb",
    "notebook",
    "bing",
    "serp",
    "markdown",
    "ocr",
    "extraction",
    "typescript"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "tsc --watch",
    "clean": "rm -rf dist examples/output",
    "rebuild": "pnpm run clean && pnpm run build",
    "prepublishOnly": "pnpm run rebuild",
    "publish:dry-run": "pnpm publish --dry-run --no-git-checks",
    "typecheck": "tsc --noEmit",
    "preversion": "pnpm run typecheck && pnpm run build",
    "release": "bumpp --commit --push --tag && pnpm publish"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/leoning60/markitdown-node.git"
  },
  "homepage": "https://github.com/leoning60/markitdown-node#readme",
  "bugs": {
    "url": "https://github.com/leoning60/markitdown-node/issues"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@huggingface/transformers": "^3.7.6",
    "@joplin/turndown-plugin-gfm": "^1.0.64",
    "@xmldom/xmldom": "^0.9.6",
    "exceljs": "^4.4.0",
    "execa": "^9.6.0",
    "fast-xml-parser": "^5.3.0",
    "jsdom": "^27.0.1",
    "mammoth": "^1.11.0",
    "node-pptx-parser": "^1.0.1",
    "pdf-ts": "^0.0.2",
    "sharp": "^0.34.4",
    "tesseract.js": "^6.0.1",
    "turndown": "^7.2.2",
    "turndown-plugin-gfm": "^1.0.2",
    "word": "^0.4.0"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.11.5",
    "@types/pako": "^2.0.4",
    "@types/turndown": "^5.0.5",
    "bumpp": "^10.3.1",
    "is-ci": "^4.1.0",
    "typescript": "^5.3.3",
    "unbuild": "^3.6.1"
  },
  "packageManager": "pnpm@9.15.1",
  "peerDependencies": {
    "unzipper": "^0.12.3",
    "youtube-transcript": "^1.2.1"
  },
  "peerDependenciesMeta": {
    "youtube-transcript": {
      "optional": true
    },
    "unzipper": {
      "optional": true
    }
  }
}